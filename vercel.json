{
    "version": 2,
    "builds": [
        {
            "src": "src/main.jsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.jsx"
        }
    ],
    "env": {
        "EXAMPLE_ENV_VAR": "example-value"
    }
}